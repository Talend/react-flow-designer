{
  "name": "react-flow-designer",
  "description": "Flow designer for react and redux",
  "version": "1.0.0",
  "main": "lib/index.js",
  "mainSrc": "src/index.js",
  "scripts": {
    "clean": "npm run clean:lib && npm run clean:coverage",
    "clean:lib": "rimraf ./lib",
    "clean:coverage": "rimraf ./coverage",
    "flow": "flow",
    "lint": "npm run lint:es",
    "lint:es": "eslint --config .eslintrc src/**",
    "prepublish": "babel -d lib ./src/ && rimraf lib/**/*.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:reset:snapshot": "jest --no-cache -u"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/react-flow-designer.git"
  },
  "author": "Axel Cateland <acateland@talend.com>",
  "license": "Apache-2.0",
  "jest": {
    "testRegex": "/src/.*\\.test(.js|.jsx)$",
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "<rootDir>/test/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg)$": "<rootDir>/test/fileMock.js"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "7.1.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-native-modules": "6.9.2",
    "babel-preset-react": "^6.22.0",
    "coveralls": "3.0.9",
    "css-loader": "3.2.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "6.7.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.7.1",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "flow-bin": "^0.41.0",
    "flow-typed": "^2.0.0",
    "immutable": "3.8.1",
    "jest-cli": "24.9.0",
    "node-sass": "3.13.0",
    "prop-types": "^15.5.10",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.0.0",
    "redux": "3.6.0",
    "redux-mock-store": "1.2.1",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.4",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "semantic-release": "15.13.31",
    "style-loader": "0.13.1",
    "webpack": "2.1.0-beta.25"
  },
  "peerDependencies": {
    "immutable": "3",
    "react": "15 || ^16.0.0",
    "react-dom": "15 || ^16.0.0",
    "react-redux": "4",
    "redux": "3",
    "reselect": "2"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "d3-drag": "1.0.0",
    "d3-interpolate": "1.1.2",
    "d3-scale": "1.0.4",
    "d3-selection": "1.0.3",
    "d3-shape": "1.0.3",
    "d3-zoom": "1.0.2",
    "invariant": "2.2.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.5.10",
    "react-immutable-proptypes": "2.1.0"
  }
}
