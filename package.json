{
  "name": "react-flow-designer",
  "description": "Flow designer for react and redux",
  "main": "lib/index.js",
  "mainSrc": "src/index.js",
  "scripts": {
    "clean": "npm run clean:lib && npm run clean:coverage",
    "clean:lib": "rimraf ./lib",
    "clean:coverage": "rimraf ./coverage",
    "lint": "npm run lint:es",
    "lint:es": "eslint --config .eslintrc src/**",
    "prepublish": "babel -d lib ./src/ && rimraf lib/**/*.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:reset:snapshot": "jest --no-cache -u",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/react-flow-designer.git"
  },
  "author": "Axel Cateland <acateland@talend.com>",
  "license": "Apache-2.0",
  "jest": {
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": "/src/.*\\.test(.js|.jsx)$",
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "<rootDir>/test/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg)$": "<rootDir>/test/fileMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.12",
    "@storybook/addon-links": "^4.0.12",
    "@storybook/addons": "^4.0.12",
    "@storybook/react": "^4.0.12",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.1",
    "babel-eslint": "8.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "3.11.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.7.1",
    "immutable": "3.8.1",
    "jest": "^23.6.0",
    "node-sass": "4.7.2",
    "prop-types": "^15.5.10",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.0.0",
    "redux": "3.6.0",
    "redux-mock-store": "1.2.1",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.4",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "semantic-release": "4.3.5",
    "style-loader": "0.19.0"
  },
  "peerDependencies": {
    "immutable": "3",
    "react": "15 || ^16.0.0",
    "react-dom": "15 || ^16.0.0",
    "react-redux": "4",
    "redux": "3",
    "reselect": "2"
  },
  "dependencies": {
    "bson-objectid": "^1.2.4",
    "classnames": "2.2.5",
    "d3-drag": "1.0.0",
    "d3-interpolate": "1.1.2",
    "d3-scale": "1.0.4",
    "d3-selection": "1.0.3",
    "d3-shape": "1.0.3",
    "d3-zoom": "1.0.2",
    "invariant": "2.2.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.5.10",
    "react-immutable-proptypes": "2.1.0"
  }
}
