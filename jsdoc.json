{
    "source": {
        "include": "./src/api",
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    },
    "docdash": {
        "typedefs": true,
        "search": true,
        "sort": true,
        "collapse": true
    }
}
